{
  "version": 1,
  "projects": {
    "web-components": {
      "root": "apps/web-components",
      "sourceRoot": "apps/web-components/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "webpackConfig": "tools/webpack/webpack.config.js",
            "outputPath": "dist/apps/web-components",
            "index": "apps/web-components/src/index.html",
            "main": "apps/web-components/src/main.ts",
            "tsConfig": "apps/web-components/tsconfig.app.json",
            "assets": [
              "apps/web-components/src/favicon.ico",
              "apps/web-components/src/assets"
            ],
            "styles": ["apps/web-components/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/web-components/src/environments/environment.ts",
                  "with": "apps/web-components/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "web-components:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "web-components:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/web-components/.eslintrc",
            "tsConfig": [
              "apps/web-components/tsconfig.app.json",
              "apps/web-components/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/web-components/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/web-components/jest.config.js",
            "tsConfig": "apps/web-components/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/web-components/src/test-setup.ts"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/web"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "web-components"
}
